	_toTree(tv,parents,key,val,i,h,uParams){
		local id
		if !parents.haskey(h) {
			id:=tv.add(h)
			parents[h]:=id
		}
		id:=tv.add(key,parents[h])
		tv.add(val,id)
		return 1
	}
